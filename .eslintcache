      delay: 400\n      })\n    const prop3 = useSpring({\n        opacity: 1,\n        from: { opacity: 0 },\n        config: {friction: 620},\n        delay: 600\n      })\n\n    useEffect(() => {\n        \n        window.addEventListener('scroll', changeNav)\n    }, [])\n\n    return (\n        <>\n        <NavbarContainer scrollNav={scrollNav}>\n            <HomeIcon style={prop1}>JM</HomeIcon>\n            <SectionLinks>\n                <LinkContainer style={prop1}>\n                    <SectionLink>About</SectionLink>\n                </LinkContainer>\n                <LinkContainer style={prop2}>\n                    <SectionLink>Projects</SectionLink>\n                </LinkContainer>\n                <LinkContainer style={prop3}>\n                    <SectionLink>Resume</SectionLink>\n                </LinkContainer>\n            </SectionLinks>\n            <MobileMenuIcon>\n                <FaBars \n                    onClick={toggleSidebar}/>\n            </MobileMenuIcon>\n        </NavbarContainer>\n        </>\n    )\n}\n\nexport default Navbar;\n","/Users/john_home/Code/Projects/portfolio_2020/src/components/Navbar/NavbarElements.js",[],"/Users/john_home/Code/Projects/portfolio_2020/src/components/Sidebar/index.js",[],"/Users/john_home/Code/Projects/portfolio_2020/src/components/Sidebar/SidebarElements.js",[],"/Users/john_home/Code/Projects/portfolio_2020/src/sections/Projects/index.js",["105","106","107","108","109"],"/Users/john_home/Code/Projects/portfolio_2020/src/sections/Projects/ProjectsElements.js",[],"/Users/john_home/Code/Projects/portfolio_2020/src/sections/About/index.js",[],"/Users/john_home/Code/Projects/portfolio_2020/src/sections/About/AboutElements.js",[],"/Users/john_home/Code/Projects/portfolio_2020/src/components/ImageGallery/index.js",[],"/Users/john_home/Code/Projects/portfolio_2020/src/components/ImageGallery/ImageGalleryElements.js",[],"/Users/john_home/Code/Projects/portfolio_2020/src/api/codeWarsAPI.js",["110"],"const axios = require('axios');\n\nconst { REACT_APP_CW_API_KEY } = process.env;\nconst user = 'JohnnyBEdge';\n\n\n\nexport const getCurrentCodewarsStats = async () => {\n    try{\n        const res = await axios.get(`https://cors-anywhere.herokuapp.com/https://www.codewars.com/api/v1/users/${user}`);\n        console.log(\"RESSS\",res)\n        return res;\n    } catch(err){\n        console.log(\"Error:\",err);\n    }\n}\n\n\n\n","/Users/john_home/Code/Projects/portfolio_2020/src/components/Codewars/index.js",["111","112"],"import React, {useEffect, useState} from 'react'\nimport { getCurrentCodewarsStats } from '../../api/codeWarsAPI';\nimport {\n    CodewarsContainer, CodewarsTitle, Stat, \n    Instructions, GetStatsBtn, Loader, StatsContainer,\n    LoaderContainer\n} from './CodewarsElements';\nimport LoadingBubbles from '../Loading';\n\nconst Codewars = () => {\n\n    const [total, setTotal] = useState(null);\n    const [overall, setOverall] = useState(null);\n    const [languages, setLanguages] = useState([]);\n    // const [cwStats, setCWStats] = useState(null);\n    const [isLoading, setIsLoading] = useState(false);\n    const [loaded, setLoaded] = useState(false);\n    const [errMsg, setErrMsg] = useState(false);\n\n    \n\n    const getStats = async() => {\n        await setIsLoading(true);\n        await getCurrentCodewarsStats().then((stats) => {\n            stats.status === 200 ? setStats(stats) : setErrMsg(true)\n        })\n        await setIsLoading(false)\n        await setLoaded(true)\n    };\n\n    // const getStats = () => {\n    //     setIsLoading(true);\n    //     getCurrentCodewarsStats().then((stats) => {\n    //         setStats(stats);\n    //         // setTotal(stats.data.codeChallenges.totalCompleted);\n    //         // console.log(stats.data.codeChallenges.totalCompleted)\n    //         // setOverall(stats.data.ranks.overall.name);\n    //         // setLanguages(...stats.data.ranks.languages);\n    //     }).then( setIsLoading(false))\n  